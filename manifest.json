{
    "version": "1.0.0",
    "manifest_version": 2,
    "name": "San proxy",
    "description": "San Proxy - My Personal Chrome Extension for using Socks Proxy.",
    "icons": {
        "128": "images/Icon-128.png",
        "48":  "images/Icon-48.png",
        "16":  "images/Icon-16.png"
    },
    "permissions": [
        "proxy",
        "tabs",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
          "js/proxy.js",
          "js/storage.js"
        ]
      },
    "browser_action": {
        "default_icon": "images/Icon-16.png",
        "default_popup": "options.html"
      },
    "options_page": "options.html",
    "short_name": "San proxy",
    "homepage_url": "https://sanweb.info"
}